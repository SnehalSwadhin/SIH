{
  "name": "संज्ञान: Sanskrit Voice-to-Text",
  "description": "Traditional Learners - \nRecord a Sanskrit audio and convert it to text",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
        "service_worker": "background.js"
    },
  "action": {
      "default_title": "Sanskrit ASR",
      "default_popup": "index.html"
  },
  "permissions": [
      "activeTab",
      "scripting",
      "tabs"
  ],
  "host_permissions": [
    "https://sanskrit.uohyd.ac.in/"
  ]
}